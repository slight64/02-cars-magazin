mixin linksList(obj,ulCls,liCls,linkCls)
  ul(class= ulCls)
    each key,val in obj
      li(class= liCls)
        a(class= linkCls href=key)=val

mixin btnList(array,ulCls,liCls,btnCls)
  ul(class= ulCls)
    each val in array
      li(class= liCls)
        button(class= btnCls)=val

mixin brandsList(obj,ulClassName,liClassName,btnClassName,radioName,labelClass)
  div(class=ulClassName)
    each val in obj
      div(class=liClassName)
        label(class=labelClass)
          img(src=`./assets/img/brands/`+val.split(' ').join('-')+`.png` alt="")
          input(type="radio" class=btnClassName name=radioName)
          |#{val}